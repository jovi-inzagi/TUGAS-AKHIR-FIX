	<queryString language="SQL">
		<![CDATA[SELECT 
    transaksi_barang.id AS id_transaksi, 
    transaksi_barang.id_customer, 
    customer_barang.nama AS nama_pelanggan, 
    barang.nama AS nama_barang, 
    barang.harga, 
    transaksi_barang.jumlah, 
    transaksi_barang.total, 
    kurir.nama_kurir, 
    kurir.jenis_kendaraan, 
    kurir.plat_nomor 
FROM 
    transaksi_barang 
JOIN 
    customer_barang ON transaksi_barang.id_customer = customer_barang.id 
JOIN 
    barang ON transaksi_barang.id_barang = barang.id 
JOIN 
    kurir ON transaksi_barang.id_kurir = kurir.id_kurir]]>
	</queryString>
